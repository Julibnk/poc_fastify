{
  "name": "fastify-template",
  "version": "1.0.0",
  "packageManager": "yarn@4.2.2",
  "engines": {
    "node": ">=20.9.0",
    "yarn": ">=3.0.0"
  },
  "main": "index.js",
  "type": "module",
  "exports": "./dist/index.js",
  "repository": "git@github.com:rtivital/fastify-template.git",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist/index.js",
    "serve": "NODE_ENV=development node -r dotenv/config dist/index.js",
    "dev": "NODE_OPTIONS='--no-warnings' NODE_ENV=development nodemon --es-module-specifier-resolution=node -r dotenv/config src/index.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "eslint src --cache",
    "typecheck": "tsc --noEmit",
    "jest": "jest",
    "prettier:test": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "npm run prettier:test && npm run typecheck && npm run lint"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "eslint": "^9.5.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.13.1"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.0",
    "zod": "^3.23.8"
  }
}
