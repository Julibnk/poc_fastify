{
  "name": "fastify-template",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "exports": "./dist/index.js",
  "repository": "git@github.com:rtivital/fastify-tutorial.git",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=development nodemon --es-module-specifier-resolution=node src/index.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "eslint src --cache",
    "typecheck": "tsc --noEmit",
    "prettier:test": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "npm run prettier:test && npm run typecheck && npm run lint"
  },
  "devDependencies": {
    "@types/node": "^20.7.1",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.2",
    "zod": "^3.22.2"
  }
}
